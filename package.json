{
  "name": "tablescript.js",
  "version": "0.3.4",
  "description": "Javascript-based Tablescript parser and interpreter",
  "url": "https://tablescript.org",
  "bin": {
    "tablescript": "./dist/bin/tablescript.js"
  },
  "main": "./dist/index.js",
  "scripts": {
    "build": "node_modules/.bin/babel src --ignore __tests__ --out-dir dist",
    "build:parser": "node_modules/.bin/pegjs --cache --output src/parser/tablescript-grammar-parser.js src/grammar/tablescript.pegjs",
    "build:tracing-parser": "node_modules/.bin/pegjs --trace --cache --output src/parser/tablescript-grammar-parser.js src/grammar/tablescript.pegjs",
    "build:template-string-parser": "node_modules/.bin/pegjs --cache --output src/parser/template-string-grammar-parser.js src/grammar/template-string.pegjs",
    "dependency-graph": "madge --image tablescript-graph.svg src/bin/tablescript.js",
    "lint": "node_modules/.bin/eslint src",
    "test": "mocha --require @babel/core --require @babel/register --require @babel/polyfill src/**/*.test.js",
    "ci:test": "mocha --reporter mocha-junit-reporter --reporter-options mochaFile=./test-results/mocha/results.xml --require @babel/core --require @babel/register --require @babel/polyfill src/**/*.test.js",
    "test:watch": "mocha --watch --require @babel/core --require @babel/register --require @babel/polyfill src/**/*.test.js",
    "postversion": "git push --tags && yarn publish . --tag $npm_package_version && git push && echo \"Successfully released version $npm_package_version!\""
  },
  "author": {
    "name": "Jamie Hale",
    "email": "jamie@smallarmyofnerds.com",
    "url": "https://smallarmyofnerds.com"
  },
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/jamiehale/tablescript.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.3",
    "@babel/core": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "@babel/register": "^7.6.2",
    "chai": "*",
    "chai-as-promised": "*",
    "chai-spies-next": "*",
    "eslint": "*",
    "eslint-config-google": "*",
    "eslint-plugin-standard": "*",
    "madge": "^3.4.4",
    "mocha": "*",
    "mocha-junit-reporter": "^1.23.1",
    "pegjs": "^0.10.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "commander": "^3.0.2",
    "pkginfo": "^0.4.1",
    "ramda": "^0.26.1"
  }
}
