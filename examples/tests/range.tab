assert(range(0) == []);

assert(range(5) == [0, 1, 2, 3, 4]);

assert(range(-5) == [0, -1, -2, -3, -4]);

assert(range(2, 7) == [2, 3, 4, 5, 6]);

assert(range(2, -5) == [2, 1, 0, -1, -2, -3, -4]);

assert(range(2, 2) == []);

assert(range(2, 7, 2) == [2, 4, 6]);

assert(range(2, -7, -2) == [2, 0, -2, -4, -6]);
