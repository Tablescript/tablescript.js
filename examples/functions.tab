foo = () => {
  1 + 2
}

bar = (x, y) => {
  x * y
}

system.print(foo())
system.print(bar(2, 3))

baz = (q) => {
  q = "overwritten"
  system.print(q)
}

q = 9
system.print(q)
baz(q)
system.print(q)

nobranch = () => {
  'nobranch'
}

system.print(nobranch())

branch = (n) => {
  if (n == 1) {
    'if'
  } else {
    'else'
  }
}

system.print(branch(1))
system.print(branch(2))

noelse = (n) => {
  if (n == 1) {
    'if'
  }
  'default'
}

system.print(noelse(1))
system.print(noelse(2))
